pipeline {
    agent { label 'JDK11 && MVN3 && NVM && PY3' } 
    stages {
        stage('giturl') {
            steps {
                git url: https://github.com/gautamshrinivas/openmrs-core.git,
                branch: 'SPRINT_1_DEV'
            }
        }
        stage ('build') {
            steps {
                sh 'mvn package'
            }
        }
    }
}